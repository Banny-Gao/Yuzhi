{
  "name": "@workspace/bazi",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
